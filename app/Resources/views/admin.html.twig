<!DOCTYPE html>
<html lang="en">

<head>
    <title>tech-events admin dash</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <script src="https://code.jquery.com/jquery-3.4.0.min.js"
        integrity="sha256-BJeo0qm959uMBGb65z40ejJYGSgR7REI4+CW1fNKwOg=" crossorigin="anonymous"></script>
    <!-- Main CSS-->
    <link rel="stylesheet" type="text/css" href="{{asset('./admin_events/css/main.css')}}">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <!-- Font-icon css-->
    <link rel="stylesheet" type="text/css"
        href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    {% block aditional_meta %}
    {% endblock %}
</head>

<body class="app sidebar-mini rtl">
    {% block header %}
    <!-- Navbar-->
    <header class="app-header"><a class="app-header__logo" href="{{path('evenements_homepage')}}">tech_events</a>
        <!-- Sidebar toggle button--><a class="app-sidebar__toggle" href="#" data-toggle="sidebar"
            aria-label="Hide Sidebar"></a>
        <!-- Navbar Right Menu-->
        <ul class="app-nav">
            <li class="app-search">
                <input class="app-search__input" type="search" placeholder="Search">
                <button class="app-search__button"><i class="fa fa-search"></i></button>
            </li>
            <!--Notification Menu-->
            <!-- User Menu-->
            <li class="dropdown"><a class="app-nav__item" href="#" data-toggle="dropdown"
                    aria-label="Open Profile Menu"><i class="fa fa-user fa-lg"></i></a>
                <ul class="dropdown-menu settings-menu dropdown-menu-right">
                    <li><a class="dropdown-item" href="#"><i class="fa fa-cog fa-lg"></i> Settings</a></li>
                    <li><a class="dropdown-item" href="#"><i class="fa fa-user fa-lg"></i> Profile</a></li>
                    <li><a class="dropdown-item" href="{{path('fos_user_security_logout')}}"><i
                                class="fa fa-sign-out fa-lg"></i> Logout</a>
                    </li>
                </ul>
            </li>
        </ul>
    </header>
    {% endblock %}
    <!-- Sidebar menu-->
    <div class="app-sidebar__overlay" data-toggle="sidebar"></div>
    <aside class="app-sidebar">
        <div class="app-sidebar__user">
            <div>
                <p class="app-sidebar__user-name">{{app.user.nomPrenom}}</p>
                <p class="app-sidebar__user-designation">admin tech-events</p>
            </div>
        </div>

        <ul class="app-menu">
            <li><a class="app-menu__item" href="{{path('admin_evenement')}}"><i
                        class="app-menu__icon fa fa-dashboard"></i><span class="app-menu__label">Evenements</span></a>
            </li>
            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-laptop"></i><span class="app-menu__label">gestion
                        evenements</span><i class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{path('admin_evenement_categories')}}"><i
                                class="icon fa fa-circle-o"></i>
                            liste
                            categories</a></li>
                    <li><a class="treeview-item" href="{{path('admin_evenement_events_signales')}}"><i
                                class="icon fa fa-circle-o"></i> evenements signalés</a>
                    </li>
                    <li><a class="treeview-item" href="{{path('admin_evenement_events_bloques')}}"><i
                                class="icon fa fa-circle-o"></i> evenements
                            bloques</a></li>
                </ul>
            </li>
            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-edit"></i><span class="app-menu__label">Fablab</span><i
                        class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{ path('fab_ajout') }}"><i
                                class="icon fa fa-circle-o"></i>Ajouter Fablab</a></li>
                    <li><a class="treeview-item" href="{{ path('fab_affichageback') }}"><i
                                class="icon fa fa-circle-o"></i>Afficher Fablabs</a></li>

                </ul>
            </li>
            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">Materiel</span><i
                        class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{ path('mat_ajout') }}"><i
                                class="icon fa fa-circle-o"></i>Ajouter Materiel</a></li>
                    <li><a class="treeview-item" href="{{ path('mat_affichageback') }}"><i
                                class="icon fa fa-circle-o"></i>Afficher Materiels</a></li>
                </ul>
            </li>
            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-file-text"></i><span class="app-menu__label">Logiciel</span><i
                        class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{ path('log_ajout') }}"><i
                                class="icon fa fa-circle-o"></i>Ajouter Logiciel</a></li>
                    <li><a class="treeview-item" href="{{ path('log_affichageback') }}"><i
                                class="icon fa fa-circle-o"></i>Afficher Logiciel</a></li>
                </ul>

            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-file-text"></i><span class="app-menu__label">Paramètres</span><i
                        class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{ path('catgmateriel_new') }}"><i
                                class="icon fa fa-circle-o"></i>Ajouter catégorie matériels</a></li>
                    <li><a class="treeview-item" href="{{ path('catgmateriel_index') }}"><i
                                class="icon fa fa-circle-o"></i>Liste catégorie matériels</a></li>
                    <li><a class="treeview-item" href="{{ path('typelog_new') }}"><i
                                class="icon fa fa-circle-o"></i>Ajouter Type logiciel</a></li>
                    <li><a class="treeview-item" href="{{ path('typelog_index') }}"><i
                                class="icon fa fa-circle-o"></i>Liste Type logiciel</a></li>




                </ul>
            </li>

            <li class="treeview"><a class="app-menu__item" href="#" data-toggle="treeview"><i
                        class="app-menu__icon fa fa-th-list"></i><span class="app-menu__label">Statistiques</span><i
                        class="treeview-indicator fa fa-angle-right"></i></a>
                <ul class="treeview-menu">
                    <li><a class="treeview-item" href="{{ path('stat') }}"><i class="icon fa fa-circle-o"></i>Afficher
                            les stats</a></li>

                </ul>
            </li>
        </ul>
    </aside>
    {% block dhiabody %}
    <main class="app-content">
        <div class="app-title">
            <div>
                <h1><i class="fa fa-dashboard"></i> {% block title_h1 %}admin dashbord{% endblock %}</h1>
                <p>{% block title_description %}gerer vos categories ainsi vos evenements{% endblock %}</p>
            </div>
            <ul class="app-breadcrumb breadcrumb">
                <li class="breadcrumb-item"><i class="fa fa-home fa-lg"></i></li>
                <li class="breadcrumb-item"><a href="#">admin</a></li>
                {% block aditional_path %}
                {% endblock %}
            </ul>
        </div>
        {% block body %}
        {% endblock %}
    </main>
    {% endblock %}
    <!-- Essential javascripts for application to work-->

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous">
    </script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous">
    </script>
    <script src="{{asset('./admin_events/js/main.js')}}"></script>
    <!-- The javascript plugin to display page loading on top-->
    <script src="{{asset('./admin_events/js/pace.min.js')}}"></script>
    <!-- Page specific javascripts-->
    <!-- Google analytics script-->
    <script type="text/javascript">
        if (document.location.hostname == 'pratikborsadiya.in') {
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
            ga('create', 'UA-72504830-1', 'auto');
            ga('send', 'pageview');
        }
    </script>
    {% block additional_js %}
    {% endblock %}
</body>

</html>